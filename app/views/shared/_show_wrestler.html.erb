      <div class="dc10 grid-line-bold-right grid-line-bottom">
        <div class="roll-number">10</div>
        <div class="gc-dc-show"><%= @wrestler.dc10 %></div>
      </div>

      <div class="dc11 grid-line-bold-right grid-line-bottom">
        <div class="roll-number">11</div>
        <div class="gc-dc-show"><%= @wrestler.dc11 %></div>
      </div>

      <!-- Center of Defensive Card -->
      <div class="dc12 grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">
        <div class="roll-number">12</div>
        <div class="gc-dc-show"><%= @wrestler.dc12 %></div>
      </div>


      <!-- Specialty Card -->
      <div class="wrestler-specialty-header grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">SPECIALTY:</div>

      <div class="wrestler-specialty-move grid-line-bold-left grid-line-bottom"><%= @wrestler.specialty %></div>

      <div class="s1 grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">1</div>
        <div class="move-show"><%= @wrestler.s1 %></div>
      </div>

      <div class="s2 grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">2</div>
        <div class="move-show"><%= @wrestler.s2 %></div>
      </div>

      <div class="s3 grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">3</div>
        <div class="move-show"><%= @wrestler.s3 %></div>
      </div>

      <div class="s4 grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">4</div>
        <div class="move-show"><%= @wrestler.s4 %></div>
      </div>

      <div class="s5 grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">5</div>
        <div class="move-show"><%= @wrestler.s5 %></div>
      </div>

      <div class="s6 grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">
        <div class="roll-number">6</div>
        <div class="move-show"><%= @wrestler.s6 %></div>
      </div>


      <!-- Sub, Tag & Priority -->
      <div class="sub grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="sub-tag-priority-header">SUB:</div>
        <div class="sub-tag-priority-show"><%= sub_and_tag_display(@wrestler.subx, @wrestler.suby) %></div>
      </div>

      <div class="tag grid-line-bold-left grid-line-bold-right grid-line-bottom">
        <div class="sub-tag-priority-header">TAG TEAM:</div>
        <div class="sub-tag-priority-show"><%= sub_and_tag_display(@wrestler.tagx, @wrestler.tagy) %></div>
      </div>

      <div class="priority grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">
        <!-- TODO: Automate Pri-T value based on TT% -->
        <div class="sub-tag-priority-header">PRIORITY:</div>
        <div class="sub-tag-priority-show"><%= @wrestler.prioritys %> / <%= @wrestler.priorityt %></div>
      </div>


      <!-- Right Side of Card -->
      <div class="wrestler-oc-header grid-line-bold-top grid-line-bold-bottom grid-line-bold-right">OFFENSIVE CARD</div>

      <div class="oc2 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">2</div>
        <div class="move-show"><%= @wrestler.oc02 %></div>
      </div>

      <div class="oc3 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">3</div>
        <div class="move-show"><%= @wrestler.oc03 %></div>
      </div>

      <div class="oc4 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">4</div>
        <div class="move-show"><%= @wrestler.oc04 %></div>
      </div>

      <div class="oc5 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">5</div>
        <div class="move-show"><%= @wrestler.oc05 %></div>
      </div>

      <div class="oc6 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">6</div>
        <div class="move-show"><%= @wrestler.oc06 %></div>
      </div>

      <div class="oc7 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">7</div>
        <div class="move-show"><%= @wrestler.oc07 %></div>
      </div>

      <div class="oc8 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">8</div>
        <div class="move-show"><%= @wrestler.oc08 %></div>
      </div>

      <div class="oc9 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">9</div>
        <div class="move-show"><%= @wrestler.oc09 %></div>
      </div>

      <div class="oc10 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">10</div>
        <div class="move-show"><%= @wrestler.oc10 %></div>
      </div>

      <div class="oc11 grid-line-bold-right  grid-line-bottom">
        <div class="roll-number">11</div>
        <div class="move-show"><%= @wrestler.oc11 %></div>
      </div>

      <div class="oc12 grid-line-bold-right">
        <div class="roll-number">12</div>
        <div class="move-show"><%= @wrestler.oc12 %></div>
      </div>



            <p>=ROPES=</p>

            <p>2 <%= @wrestler.ro02 %></p>
            <p>3 <%= @wrestler.ro03 %></p>
            <p>4 <%= @wrestler.ro04 %></p>
            <p>5 <%= @wrestler.ro05 %></p>
            <p>6 <%= @wrestler.ro06 %></p>
            <p>7 <%= @wrestler.ro07 %></p>
            <p>8 <%= @wrestler.ro08 %></p>
            <p>9 <%= @wrestler.ro09 %></p>
            <p>10 <%= @wrestler.ro10 %></p>
            <p>11 <%= @wrestler.ro11 %></p>
            <p>12 <%= @wrestler.ro12 %></p>

            <p><%= @wrestler.set %></p>
            <p>SPWG Card Maker</p>
        </div> <!-- End of wrestler-show-container -->

    </div> <!-- End of wrestler-show -->

    <%= render 'shared/form_stats', wrestler: wrestler, class: "wrestler-stats" %>
</div> <!-- End of wrestler-display-container -->




    