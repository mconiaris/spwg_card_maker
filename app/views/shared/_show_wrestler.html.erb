<div class="wrestler-display-container">
  <div class="wrestler-show">
    <div class="wrestler-show-container">
      <!-- Left Side of Card -->
      <div class="wrestler-name grid-line-bold"><%= @wrestler.name %></div>

      <div class="wrestler-gc-header grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">GENERAL CARD</div>

      <!-- Left Side of GC -->
      <div class="gc2 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">2</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc02 %>
        </div>
      </div>

      <div class="gc3 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">3</div>
        <div class="gc-dc-show"><%= @wrestler.gc03 %></div>
      </div>

      <div class="gc4 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">4</div>
        <div class="gc-dc-show"><%= @wrestler.gc04 %></div>
      </div>

      <div class="gc5 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">5</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc05 %>
        </div>
      </div>

      <div class="gc6 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">6</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc06 %>
        </div>
      </div>

      <!-- Right Side of GC -->
      <div class="gc7 grid-line-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">7</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc07 %>
        </div>
      </div>

      <div class="gc8 grid-line-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">8</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc08 %>
        </div>
      </div>

      <div class="gc9 grid-line-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">9</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc09 %>
        </div>
      </div>

      <div class="gc10 grid-line-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">10</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc10 %>
        </div>
      </div>

      <div class="gc11 grid-line-left grid-line-bold-right grid-line-bottom">
        <div class="roll-number">11</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc11 %>
        </div>
      </div>

      <!-- Center of General Card -->
      <div class="gc12 grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">
        <div class="roll-number">12</div>
        <div class="gc-dc-show">
          <%= @wrestler.gc12 %>
        </div >
      </div>


      <!-- Defensive Card -->
      <div class="wrestler-dc-header grid-line-bold-left grid-line-bold-right grid-line-bold-bottom">DEFENSIVE CARD</div>

      <!-- Left of Defensive Card -->
      <div class="dc2 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">2</div>
        <div class="gc-dc-show"><%= @wrestler.dc02 %></div>
      </div>

      <div class="dc3 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">3</div>
        <div class="gc-dc-show"><%= @wrestler.dc03 %></div>
      </div>

      <div class="dc4 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">4</div>
        <div class="gc-dc-show"><%= @wrestler.dc04 %></div>
      </div>

      <div class="dc5 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">5</div>
        <div class="gc-dc-show"><%= @wrestler.dc05 %></div>
      </div>

      <div class="dc6 grid-line-bold-left grid-line-bottom">
        <div class="roll-number">6</div>
        <div class="gc-dc-show"><%= @wrestler.dc06 %></div>
      </div>

      <!-- Right of Defensive Card -->
      <div class="dc7 grid-line-bold-right grid-line-bottom">
        <div class="roll-number">7</div>
        <div class="gc-dc-show"><%= @wrestler.dc07 %></div>
      </div>

      <div class="dc8 grid-line-bold-right grid-line-bottom">
        <div class="roll-number">8</div>
        <div class="gc-dc-show"><%= @wrestler.dc08 %></div>
      </div>

      <div class="dc9 grid-line-bold-right grid-line-bottom">
        <div class="roll-number">9</div>
        <div class="gc-dc-show"><%= @wrestler.dc09 %></div>
